{
  "name": "chronos",
  "version": "5.1.1",
  "description": "Chronos Microservice Debugger",
  "main": "./build/Main.js",
  "scripts": {
    "build": "webpack",
    "start": "webpack-dev-server",
    "nodemon": "nodemon server/server.js",
    "electron": "electron .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:electron": "jest --detectOpenHandles ./__tests__/electron/*",
    "test:app": "jest ./__tests__/app/*",
    "both": "npm run start & sleep 5 && npm run electron",
    "both:tsc": "nodemon --watch ./app & npm run start & sleep 5 && npm run electron",
    "both:dev": "npm run start & nodemon --watch ./electron --exec 'npm run electron'",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "create-installer-mac": "electron-installer-dmg ./release-builds/chronos-darwin-x64/chronos.app/ Chronos",
    "delete": "rm -rf dist & rm -rf build",
    "reinstall": "rm -rf node_modules && npm install",
    "prepare": "tsc -v && tsc -p . && npm run build",
    "rebuild": "npm run delete && npm run prepare"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "keywords": [
    "distributed tracing",
    "tracing"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "chai": "^4.2.0",
    "chart.js": "^2.8.0",
    "d3": "^5.16.0",
    "electron": "^6.1.11",
    "enzyme-adapter-react-16": "^1.15.5",
    "express": "^4.17.1",
    "moment": "^2.27.0",
    "mongodb": "^3.5.7",
    "mongoose": "^5.9.13",
    "pg": "^7.18.2",
    "plotly.js": "^1.54.1",
    "plotly.js-dist": "^1.54.1",
    "react": "16.10.1",
    "react-dom": "16.10.1",
    "react-graph-vis": "^1.0.5",
    "react-plotly.js": "^2.4.0",
    "react-router-dom": "^5.2.0",
    "webdriverio": "^6.1.25"
  },
  "devDependencies": {
    "@babel/core": "^7.10.1",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.10.1",
    "@babel/preset-react": "^7.9.4",
    "@jest-runner/electron": "^2.0.3",
    "@types/chai": "^4.2.11",
    "@types/electron": "^1.6.10",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^26.0.4",
    "@types/material-ui": "^0.21.8",
    "@types/mongoose": "^5.7.28",
    "@types/node": "^14.0.14",
    "@types/pg": "^7.14.3",
    "@types/react": "^16.9.36",
    "@types/react-dom": "^16.9.8",
    "@types/react-plotly.js": "^2.2.4",
    "@types/react-router-dom": "^5.1.5",
    "@types/react-test-renderer": "^16.9.2",
    "awesome-typescript-loader": "^5.2.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.5.3",
    "electron-devtools-installer": "^3.0.0",
    "electron-installer-dmg": "^3.0.0",
    "electron-packager": "^15.1.0",
    "enzyme": "^3.11.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^4.3.0",
    "html-webpack-plugin": "^3.2.0",
    "ify-loader": "^1.1.0",
    "image-webpack-loader": "^6.0.0",
    "jest": "^26.1.0",
